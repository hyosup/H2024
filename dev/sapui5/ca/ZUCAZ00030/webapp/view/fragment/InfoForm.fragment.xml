<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield"
>
	<smartForm:SmartForm id="idForm" editable="{viewConfig>/isEdit}">
		<smartForm:Group>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Objid}" />
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Subobjid}" />
			</smartForm:GroupElement>  
			<smartForm:GroupElement>
				<smartField:SmartField value="{Title}" />
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>